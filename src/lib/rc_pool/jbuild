(library
 ((name rc_pool)
  (public_name rc_pool)
  (flags (:standard -short-paths -warn-error -58))
  (library_flags (-linkall))
  (inline_tests)
  (libraries
    ( snark_params core_kernel ))
  (preprocess (pps (ppx_jane ppx_deriving.eq bisect_ppx -conditional)))
  (synopsis "A pool for reference-counting large things")))

