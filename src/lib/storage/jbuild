(jbuild_version 1)

(library
 ((name storage)
  (public_name storage)
  (flags (:standard -short-paths -warn-error -58))
  (inline_tests)
  (library_flags (-linkall))
  (libraries (core async async_extra logger))
  (preprocess (pps (ppx_jane bisect_ppx -conditional)))
  (synopsis "Storage module checksums data and stores it")))

